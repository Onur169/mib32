{
    "name": "climatestrike/api",
    "version": "1.0.0",
    "description": "Climatestrike REST-Api",
    "require": {
        "slim/slim": "^4.0",
        "nyholm/psr7": "^1.3",
        "nyholm/psr7-server": "^1.0",
        "guzzlehttp/psr7": "^1.7",
        "http-interop/http-factory-guzzle": "^1.0",
        "laminas/laminas-diactoros": "^2.5",
        "php-di/slim-bridge": "^3.1",
        "intervention/image": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Middleware\\": "src/middleware/",
            "App\\Exception\\": "src/exception/",
            "App\\Routes\\": "src/routes/",
            "App\\Classes\\": "src/classes/"
        }
    }
}
